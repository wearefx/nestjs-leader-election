    "eslint-plugin-promise": "5.1.0",
    "husky": "4.3.8",
    "jest": "27.0.6",
    "lint-staged": "11.0.0",
    "prettier": "2.3.2",
    "ts-jest": "27.0.3",
    "typescript": "4.3.5"
  }
