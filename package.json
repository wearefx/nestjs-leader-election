    "@fxd-dev-util/eslint-config-base": "1.0.0",
    "@types/cron": "1.7.2",
    "@types/jest": "26.0.23",
    "@types/prettier": "2.3.1",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.28.1",
